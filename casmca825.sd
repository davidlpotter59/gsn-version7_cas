  Unsigned Ascii Number L_MEDICAL_EXPENSE;
  Unsigned Ascii Number L_VEHICLE_TOTALS;

Initialization
  Deletefile (L_FILE_NAME);

Update

Access SFPNAME,
       Set SFPNAME:POLICY_NO    = SFPPRINT1:POLICY_NO,
           SFPNAME:POL_YEAR     = SFPPRINT1:POL_YEAR,
           SFPNAME:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

Access sfpmaster,
       Set sfpmaster:POLICY_NO    = SFPPRINT1:POLICY_NO,
           sfpmaster:POL_YEAR     = SFPPRINT1:POL_YEAR,
           sfpmaster:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

Access CAPVEHICLE,
       Set CAPVEHICLE:POLICY_NO = SFPPRINT1:POLICY_NO,
           CAPVEHICLE:POL_YEAR  = SFPPRINT1:POL_YEAR,
           CAPVEHICLE:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

Access CAPPIP,
       Set CAPPIP:POLICY_NO = SFPPRINT1:POLICY_NO,
           CAPPIP:POL_YEAR  = SFPPRINT1:POL_YEAR,
           CAPPIP:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

l_state = capvehicle:state
I_CODE = "MCA825"
l_form = 0

Access SFSEND, Set SFSEND:COMPANY_ID = sfpname:COMPANY_ID,
                   SFSEND:STATE = L_STATE ,
                   SFSEND:LINE_OF_BUSINESS = sfpname:LINE_OF_BUSINESS,
                   SFSEND:FORM = L_FORM,
                   sfsend:code = i_code, Generic

Repeat Begin
    Exit If SFSEND:COMPANY_ID <> sfpname:COMPANY_ID Or
            SFSEND:STATE <> L_STATE Or
            SFSEND:LINE_OF_BUSINESS <> sfpname:LINE_OF_BUSINESS Or
            SFSEND:FORM <> L_FORM or
            sfsend:code <> i_code

/* update sfpprint2 with the mandatory endorsements that should print on the
   common dec page */

    If (SFPNAME:EFF_DATE >= SFSEND:EFF_DATE And
       (SFPNAME:EFF_DATE <= SFSEND:EXP_DATE Or
       SFSEND:EXP_DATE = 0)) Then
        Begin
        If SFSEND:RENEWAL = 1 And
           SFPMASTER:TRANS_CODE = 14 Then
            Begin
            If ((SFPNAME:EFF_DATE <= SFSEND:EXP_DATE Or
               SFSEND:EXP_DATE = 0) And
               SFPNAME:EFF_DATE >= SFSEND:RENEWAL_EFF_DATE and
               (sfpname:eff_date <= sfsend:renewal_exp_date or
               sfsend:renewal_exp_date = 0)) Then
                Begin
                l_edition_print = sfsend:edition_print
                l_form_print    = sfpend:form_print
                End
            End
        Else
            If SFSEND:NEW = 1 And
               SFPMASTER:TRANS_CODE = 10 Then
                Begin
                If ((SFPNAME:EFF_DATE <= SFSEND:EXP_DATE Or
                   SFSEND:EXP_DATE = 0) And
                   SFPNAME:EFF_DATE >= SFSEND:NEW_EFF_DATE and
                   (sfpname:eff_date <= sfsend:new_exp_date or
                   sfsend:new_exp_date = 0)) Then
                    Begin
                    l_edition_print = sfsend:edition_print
                    l_form_print    = sfsend:form_print
                    End
                End
            Else
            if sfsend:new = 0 and
               sfsend:renewal = 0 then
                Begin
                l_edition_print = sfsend:edition_print
                l_form_print    = sfsend:form_print
                End

        End

    Next SFSEND
    End

access sfsoptend,
   set sfsoptend:company_id       = sfpname:company_id,
       sfsoptend:state            = l_state,
       sfsoptend:line_of_business = sfpname:line_of_business,
       sfsoptend:code             = i_code, generic

l_edition = 0
l_copyright = ""
while sfsoptend:company_id       = sfpname:company_id and
      sfsoptend:state            = l_state and
      sfsoptend:line_of_business = sfpname:line_of_business and
      sfsoptend:code             = i_code
  begin
  if (sfpname:eff_date >= sfsoptend:eff_date and
     (sfpname:eff_date < sfsoptend:exp_date or
     sfsoptend:exp_date = 0)) then
        begin
        l_form_print    = sfsoptend:form_Print
        l_edition_print = sfsoptend:edition_print
        l_edition       = sfsoptend:edition
        l_copyright     = sfsoptend:copyright
        end
    next sfsoptend
  end

